[
chart: {
  type: "pieChart",
  height: 500,
  x: function (d){return d.song;},
  y: function (d){return d.plays;},
  showLabels: true,
  duration: 500,
  labelThreshold: 0.01,
  labelSunbeamLayout: true,
  legend: {
    margin: {
      top: 5,
      right: 35,
      bottom: 5,
      left: 0
    }
  }
}
title: {
  enable: true,
  text: "Songs Played: Service rendered",
  className: "h4",
  css:
  {
    width: 0,
    textAlign: "center"
  }
]
